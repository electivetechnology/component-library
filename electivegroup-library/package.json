    "@types/redux-mock-store": "^1.0.2",
    "@types/styled-components": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.2",
    "electivegroup-library": "^1.0.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
