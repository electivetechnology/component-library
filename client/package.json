    "dayjs": "1.10.x",
    "immer": "6.0.x",
    "lodash": "4.17.x",
    "react": "17.0.x",
    "react-phone-input-mui": "2.9.x",
    "react-phone-input-2": "2.13.x",
    "react-csv-downloader": "1.8.x",
    "react-dom": "17.0.x",
    "react-redux": "7.2.x",
    "react-router-dom": "5.1.x",
    "react-scripts": "4.0.x",
