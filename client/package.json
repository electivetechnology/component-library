{
  "name": "electivegroup-component-library",
  "version": "1.0.5",
  "engines": {
    "node": ">=10"
  },
  "description": "Styles Guide and common components for App",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.tsx",
  "author": "Chris Dixon <chrisd@recii.io>",
  "license": "UNLICENSED",
  "repository": {
    "url": "https://gitlab.et-ns.net/connect/component-library",
    "type": "git"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint",
    "test:unit": "cross-env CI=1 react-scripts test --env=jest-environment-jsdom-sixteen",
    "test:watch": "react-scripts test --env=jest-environment-jsdom-sixteen",
    "storybook": "start-storybook -p 6006 --no-dll",
    "build-storybook": "build-storybook --no-dll",
    "docker-test": "react-scripts test --env=jest-environment-jsdom-sixteen --forceExit --colors --detectOpenHandles --runInBand --watchAll=false",
    "format": "yarn run prettier --write 'src/**/*.{js,jsx,ts,tsx}'"
  },
  "peerDependencies": {
    "@material-ui/core": "^4.9.x",
    "@material-ui/icons": "^4.9.x",
    "@material-ui/lab": "^4.0.0-alpha.x",
    "@material-ui/pickers": "^3.2.x",
    "@types/lodash": "^4.14.x",
    "@types/node": "^12.12.x",
    "@types/react": "^16.9.x",
    "@types/react-dom": "^16.9.x",
    "@types/react-redux": "^7.1.x",
    "@types/styled-components": "^5.1.x",
    "immer": "^6.0.x",
    "lodash": "^4.17.x",
    "react": "^17.0.x",
    "react-csv-downloader": "^1.8.x",
    "react-dom": "^17.0.x",
    "react-redux": "^7.2.x",
    "react-scripts": "^4.0.x",
    "redux": "^4.0.x",
    "styled-components": "^5.1.x",
    "typescript": "^4.0.x"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@material-ui/pickers": "^3.2.10",
    "@storybook/addon-a11y": "^6.0.28",
    "@storybook/addon-backgrounds": "^5.3.18",
    "@storybook/addon-console": "^1.2.2",
    "@storybook/addon-essentials": "^6.0.28",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/addon-links": "^6.0.28",
    "@storybook/addon-viewport": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/node-logger": "^6.0.28",
    "@storybook/preset-create-react-app": "^3.1.4",
    "@storybook/react": "^6.0.28",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^25.1.4",
    "@types/lodash": "^4.14.165",
    "@types/node": "^14.14.6",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@types/react-redux": "^7.1.11",
    "@types/redux-mock-store": "^1.0.2",
    "@types/styled-components": "^5.1.4",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "gh-pages": "^2.2.0",
    "husky": "^4.2.5",
    "immer": "^7.0.14",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "lodash": "^4.17.20",
    "microbundle-crl": "^0.13.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "pretty-quick": "^2.0.1",
    "react": "^17.0.1",
    "react-csv-downloader": "^2.2.0",
    "react-dom": "^17.0.1",
    "react-is": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-scripts": "4.0.0",
    "redux": "^4.0.5",
    "redux-mock-store": "^1.5.4",
    "styled-components": "^5.2.1",
    "typescript": "4.0.5"
  },
  "eslintConfig": {
    "extends": "react-app",
    "overrides": [
      {
        "files": [
          "**/*.stories.tsx"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]

  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "files": [
    "dist"
  ]
}
