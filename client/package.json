{
  "name": "electivegroup-component-library",
  "version": "1.3.9",
  "engines": {
    "node": ">=10"
  },
  "description": "Styles Guide and common components for App",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.tsx",
  "author": "Chris Dixon <chrisd@recii.io>",
  "license": "UNLICENSED",
  "repository": {
    "url": "https://gitlab.et-ns.net/connect/component-library",
    "type": "git"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "prepare": "run-s test",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint",
    "test:unit": "cross-env CI=1 react-scripts test --env=jest-environment-jsdom-sixteen",
    "test:watch": "react-scripts test --env=jest-environment-jsdom-sixteen",
    "storybook": "start-storybook -p 6006 --no-dll",
    "build-storybook": "build-storybook --loglevel error",
    "docker-test": "react-scripts test --env=jest-environment-jsdom-sixteen --forceExit --colors --detectOpenHandles --runInBand --watchAll=false",
    "format": "yarn run prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "chromatic": "chromatic --project-token 6vtzo56j0w --exit-zero-on-changes"
  },
  "peerDependencies": {
    "@date-io/dayjs": "1.3.x",
    "@material-ui/core": "4.9.x",
